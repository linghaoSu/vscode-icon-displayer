{
  "publisher": "linghaosu",
  "author": {
    "email": "slh001@live.cn",
    "name": "linghao.su"
  },
  "name": "vscode-icon-displayer",
  "displayName": "VSCode Icon Displayer",
  "version": "0.0.3",
  "packageManager": "pnpm@7.0.1",
  "description": "",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/linghaoSu/vscode-icon-displayer"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint .",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "release": "bumpp --commit --push --tag",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm build"
  },
  "dependencies": {
    "opentype.js": "^1.3.4",
    "ohmyfetch": "^0.4.21",
    "wawoff2": "^2.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/opentype.js": "^1.3.4",
    "@types/wawoff2": "1.0.0",
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.41",
    "@types/vscode": "^1.68.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "pnpm": "^7.1.9",
    "tsup": "6.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.11",
    "vitest": "^0.14.1",
    "vsce": "^2.9.1"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "IconDisplayer",
      "properties": {
        "vscode-icon-displayer.iconUrl": {
          "type": "string",
          "description": "The url of the icon font file",
          "default": "https://cdn.jsdelivr.net/npm/@dao-style/core@latest/dist/font/DaoCloudIconFont/DaoCloudIconFont.woff2"
        },
        "vscode-icon-displayer.styleUrl": {
          "type": "string",
          "description": "The url of the icon font css file",
          "default": "https://cdn.jsdelivr.net/npm/@dao-style/core@latest/dist/style.css"
        },
        "vscode-icon-displayer.mode": {
          "type": "string",
          "enum": [
            "enable",
            "disable",
            "auto"
          ],
          "default": "auto",
          "description": "enable: always show icon\n\ndisable: never show icon\n\nauto: show icon when the file is not a text file"
        }
      }
    }
  }
}
